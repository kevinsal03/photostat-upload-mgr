CREATE TABLE dev (
	id INT AUTO_INCREMENT,
	uuid VARCHAR(255),
	active BOOLEAN,
	`created-by` VARCHAR(255),
	`created-at` DATETIME,
	PRIMARY KEY (id)
);

CREATE TABLE prod (
	id INT AUTO_INCREMENT,
	uuid VARCHAR(255),
	active BOOLEAN,
	`created-by` VARCHAR(255),
	`created-at` DATETIME,
	PRIMARY KEY (id)
);
